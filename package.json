    "nyc": "^15.1.0",
    "standard-version": "^9.1.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.25",
    "typedoc-plugin-markdown": "^3.5.0",
    "typescript": "^4.1.5"
