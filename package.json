{
  "name": "@jlguenego/graph",
  "version": "0.0.0",
  "private": false,
  "description": "Graph theory library",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "module": "build_esm/src/index.js",
  "files": [
    "build/src",
    "build_esm/src"
  ],
  "license": "ISC",
  "keywords": [
    "graph"
  ],
  "scripts": {
